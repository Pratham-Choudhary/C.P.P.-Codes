#include <bits/stdc++.h>
using namespace std;

int main()
{
	int n,num,reminder,reverse=0;
	cout<<"Enter the number to be reversed: ";
	cin>>num;
	n=num;
	do
	{
		reminder=n%10;
		reverse=reverse*10+reminder;
		n = n/10;
	}
	while(n!=0);
	{
	cout<<"\nThe Reverse is: "<<reverse;
    if (n==reverse)
    {
        cout<<"\nPalindrome";
    }
    else{
        cout<<"\nNot Palindrome";
    }
	}
}
