#include <bits/stdc++.h>
using namespace std;

int hcf(int a,int b)
{
	int h;
	for( int i =1; i<=b; i++)
    {
        if (a % i == 0 && b % i == 0)
        {
        	h = i;
        }
    }
    return h;
}

int main()
{
    int n1,n2;
    cout<<"Enter the Numbers: ";
    cin>>n1>>n2;
    cout<<"HCF = "<<hcf(n1,n2);
    return 0;
}
