#include <bits/stdc++.h>
using namespace std;

int main()
{
	char c;
	bool vol, bvol; 
	cout<<"Enter the Letter: ";
	cin>>c;
	vol=(c=='a' || c=='e' || c=='i' || c=='o' || c=='u');
	bvol=(c=='A' || c=='E' || c=='I' || c=='O' || c=='U');
	if(vol || bvol){
		cout<<"\nThe Letter is Vowel.";
	}
	else{
		cout<<"\nThe Letter is Consonent";
	}
}
