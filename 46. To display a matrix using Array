#include <bits/stdc++.h>
using namespace std;

int main()
{
	int a[3][3],i,j;
	cout<<"Enter the elements of a Matrix A: "<<endl;
	for(i = 0; i<3 ; i++)
	for(j = 0; j<3 ; j++)
	cin>>a[i][j];
	
	cout<<"Matrix A is:"<<endl;
	for(i =0; i<3 ;i++)
	{
		for(j=0; j<3; j++)
		{
			cout<<a[i][j];
		}
		cout<<"\n";
	}
	return 0;
}
