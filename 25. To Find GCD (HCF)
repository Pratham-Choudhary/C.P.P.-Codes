#include <bits/stdc++.h>
using namespace std;

int main()
{
    int n1,n2,h;
    cout<<"Enter the Numbers: ";
    cin>>n1>>n2;
    if (n2>n1)
    {
        swap(n1,n2);
    }
    for( int i =1; i<=n2; i++)
    {
        if (n1 % i == 0 && n2 % i == 0)
        {
            h = i;
        }
    }
    cout<<n1<<n2<<"HCF = "<<h;
}
