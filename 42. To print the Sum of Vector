#include <bits/stdc++.h>
using namespace std;

int sum_vector(vector<int>A)
{
	int j=0, sum =0;
    for(int i=0 ; i<A.size() ; i++)
    {
        j =A[i]%10;
        sum += j;
        j =A[i]/10;
    }
    cout<<"Sum of Vector A:- "<<sum;
    return 0;
}

int main()
{
    vector<int>A ={1,5,8,2,3,9,7};
    sum_vector(A);
    return 0;
}
